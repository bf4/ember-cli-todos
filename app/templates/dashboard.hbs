<h2>The Dashboard</h2>
{{top-nav routePrefix="dashboard" currentRouteName=application.currentRouteName}}

<table class="hoverable sortable-table">
  <thead>
    <tr>
      <th data-field="startDate">
        <span {{action "setSortBy" "startDate"}}>
          <i class="mdi-content-sort"></i>
          Start Date
        </span>
      </th>
      <th data-field="endDate">
        <span {{action "setSortBy" "endDate"}}>
          <i class="mdi-content-sort"></i>
          End Date
        </span>
      </th>
    </tr>
  </thead>
  <tbody>
    {{#each tableContent as |row|}}
      <tr>
        <td>{{row.startDate}}</td>
        <td>{{row.endDate}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
{{outlet}}
